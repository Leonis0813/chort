class ApplicationController {
  + searchPayments(conditions : String[]) : void
  + getCategories() : String[]
}

class ViewController {
  - {static} FRAME_WIDTH : int = 1440
  - {static} FRAME_HEIGHT : int = 760
  - {static} TABLE_WIDTH : int = 700
  - {static} TABLE_HEIGHT : int = 660
  + {static}  createConstraints(x : int, y : int, width : int, height : int) : GridBagLayout
  + resetMessageField() : void
  + noticeInputError(ids : ArrayList) : void
  + resetTables() : void
  + showPayments(paymentList : ArrayList) : void
}

abstract class PaymentTable {
  - tableModel : DefaultTableModel
  - sorter : TableRowSorter
  + {static} columnNames : String[]
  + {static} columnWidths : int[]
  + {static} columnClasses : Class[]
  + {static} DATE : int = 0
  + {static} CONTENT : int = 1
  + {static} CATEGORY : int = 2
  + {static} PRICE : int = 3
  + {static} PAYMENT_TYPE : int = 4
  + addPayment(payment : String[]) : void
}

class IncomeTable
class ExpenseTable

class SearchPanel {
  - {static} DATE_BEFORE : int = 0
  - {static} DATE_AFTER : int = 1
  - {static} CONTENT : int = 2
  - {static} CONTENT_TYPE : int = 3
  - {static} CATEGORY : int = 4
  - {static} PRICE_UPPER : int = 5
  - {static} PRICE_LOWER : int = 6
  - ctrl : ApplicationController
  - searchButton : JButton
  + showWrongInput(ids : ArrayList) : void
  + actionPerformed(event : ActionEvent) : void
}

class MessageField {
  - textField : JTextField
  + showMessage(message : String) : void
}

abstract class Form {
  * label : JLabel
  * checker : JLabel
}

class DateForm {
  - {static} fieldSize : int = 8
  - dateBefore : JTextField
  - dateAfter : JTextField
  + getDateBefore() : String
  + getDateAfter() : String
  + check() : void
  + uncheck() : void
}

class ContentForm {
  - {static} fieldSize : int = 20
  - {static} types : String[] = {"と一致する", "を含む"}
  - content : JTextField
  - contentType : JComboBox
  + getContent() : String
  + getContentType() : String
}

class CategoryForm {
  - {static} FIELD_SIZE : int = 10
  - {static} SELECT_BUTTON_WIDTH : int = 20
  - {static} SELECT_BUTTON_HEIGHT : int = 20
  - {static} SELECT_BUTTON_LABEL : String = "▼"
  - ctrl : ApplicationController
  - category : JTextField
  - selectButton : JButton
  + setCategory(categoryName : String) : void
  + getCategory() : String
  + actionPerformed(event : ActionEvent) : void
}

class CategoryDialog {
  - {static} DIALOG_WIDTH : int = 300
  - {static} DIALOG_HEIGHT : int = 400
  - {static} SCROLL_PANE_WIDTH : int = 250
  - {static} SCROLL_PANE_HEIGHT : int = 350
  - {static} BUTTON_WIDTH : int = 150
  - {static} DIALOG_HEIGHT : int = 20
  - {static} BUFFER : int = 20
  - categories : JRadioButton[]
  - OK : JButton
  - cancel : JButton
  + actionPerformed(event : ActionEvent) : void
}

class PriceForm {
  - {static} fieldSize : int = 8
  - priceUpper : JTextField
  - priceLower : JTextField
  + getPriceUpper() : String
  + getPriceLower() : String
  + check() : void
  + uncheck() : void
}

class InputChecker {
  - datePattern : Pattern
  - pricePattern : Pattern
  - dateFormat : DateFormat
  + checkDate(date : String) : boolean
  + checkPrice(price : String) : boolean
}

class HTTPClient {
  - con : HttpUrlConnection
  - url : URL
  - {static} host : String
  - port : String
  - {static} application_id : String
  - {static} application_key : String
  - query : String
  - response : ArrayList
  + getPayments(condition : HashMap) : ArrayList
  + getCategories(keyword : String) : ArrayList
  - sendRequest(method : String, path : String, query : String) : String
  - credential() : String
}

ApplicationController "1" -left-> "1" InputChecker
ApplicationController "1" -right-> "1" HTTPClient
ApplicationController "1" -down-> "1" ViewController
ViewController "1" -left-> "2" PaymentTable
ViewController "1" -down-> "1" SearchPanel
ViewController "1" -right-> "1" MessageField
PaymentTable <|-- IncomeTable
PaymentTable <|-- ExpenseTable
SearchPanel "1" *-- "1" DateForm
SearchPanel "1" *-- "1" ContentForm
SearchPanel "1" *-- "1" CategoryForm
SearchPanel "1" *-- "1" PriceForm
CategoryForm "1" *-- "1" CategoryDialog
Form <|-- DateForm
Form <|-- ContentForm
Form <|-- CategoryForm
Form <|-- PriceForm

autonumber

"import/rates" -> Dir : mktmpdir

loop for each month
  alt ファイルが存在する
    "import/rates" -> ZipUtil : read
  else
    "import/rates" -> FileUtils : cp
  end
end

loop for each date
  "import/rates" -> ImportUtil : target_files

  loop ファイル数分繰り返す
    "import/rates" -> CSV : open
    "import/rates" -> Rate : load_data
  end
end

"import/rates" -> FileUtils : rm_r

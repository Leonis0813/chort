class ApplicationRecord {
  + {static} load_data(file_name : String) : void
  + {static} dump(file_name : String, date : Date) : void
}

class Rate {
  - time : Datetime
  - pair : String
  - bid : double
  - ask : double
  + to_csv() : Array
}

class CandleStick {
  - from : Datetime
  - to : Datetime
  - pair : String
  - time_frame : String
  - open : Float
  - close : Float
  - high : Float
  - low : Float
  + to_csv() : Array
}

class MovingAverage {
  - time : Datetime
  - pair : String
  - time_frame : String
  - period : Integer
  - value : Float
  + to_csv() : Array
}

class ImportUtil {
  + {static} target_files(csv_dir : String, date_string : String) : Array
}

class ZipUtil {
  + {static} read(tar_gz_file : String, output_dir : String) : void
  + {static} write(tar_gz_file : String, base_dir : String, src_files : Array) : void
}

ApplicationRecord <|-- Rate
ApplicationRecord <|-- CandleStick
ApplicationRecord <|-- MovingAverage

class Rate {
  - time : Datetime
  - pair : String
  - bid : double
  - ask : double
}

class CandleStick {
  - from : Datetime
  - to : Datetime
  - pair : String
  - time_frame : String
  - open : Float
  - close : Float
  - high : Float
  - low : Float
}

class MovingAverage {
  - time : Datetime
  - pair : String
  - time_frame : String
  - period : Integer
  - value : Float
}

autonumber

aggregate -> Entry : pluck

autonumber stop
aggregate <-- Entry

autonumber resume
aggregate -> Feature : pluck

autonumber stop
aggregate <-- Feature

loop
  autonumber resume
  aggregate -> Race : find

  autonumber stop
  aggregate <-- Race

  autonumber resume
  aggregate -> Entry : find

  autonumber stop
  aggregate <-- Entry

  autonumber resume
  aggregate -> Horse : find

  autonumber stop
  aggregate <-- Horse

  autonumber resume
  aggregate -> Feature : create!
end
